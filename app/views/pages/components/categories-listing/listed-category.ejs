
<a href='/?category=<%=category.key%>' class='listed-category'>
    <%- include(`../../../ssrUtils/responsiveImage`, {
        url: `/articles/image/${category.imageUrl}`,
        imageSizes: [100, 200, 300, 400],
        classList: 'listed-category__image cursor-change center-background img-fit',
        data: category.imageUrl,
        id: null
      }) %>
    <div class='listed-category__label'><%=category.displayValue %></div>
</a>