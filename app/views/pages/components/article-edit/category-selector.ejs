<link rel="stylesheet" href="/stylesheets/category-selector.css">

<div>
    <h3>Select a category</h3>
    <select class='category-selector' name="categories" multiple required>
       <% categories.forEach(category => { %>
        <option value="<%=category.key%>"><%=category.displayValue%></option>
       <% }) %>
    </select>
</div>